{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "React": true,
    "wx": true,
    "$": true,
    "lib": true
  },
  "rules": {
    "max-len": [ "error", 120 ],
    "comma-dangle": [ "error", "never" ],
    "eol-last": [ "off" ],
    "react/prefer-stateless-function": [ "off" ]
  }
}